  <ul>
    {{#each model as |legislator|}}
      <li>{{legislator.title}}. {{legislator.first_name}} {{legislator.last_name}}<br>Contact: <a href=  {{legislator.contact_form}}>{{legislator.contact_form}}</a></li>
      Social Media:<br>
        {{#if legislator.facebook_id}}
          <a href='http://www.facebook.com/{{legislator.facebook_id}}'>Facebook</a><br>
        {{/if}}
        {{#if legislator.twitter_id}}
          <a href='http://www.twitter.com/{{legislator.twitter_id}}'>Twitter</a><br>
        {{/if}}
        {{#if legislator.youtube_id}}
          <a href='http://www.youtube.com/{{legislator.youtube_id}}'>Youtube</a><br>
        {{/if}}
    {{/each}}
  </ul>
